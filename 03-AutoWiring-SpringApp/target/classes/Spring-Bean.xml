<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<!-- If our scope is prototype then only when will call getBean() object will be created
if our scope is singleton then by default objects will be created when IOC container starts -->

<bean id = "abc" class = "com.learn.DieselEngine"/>
<bean id = "car" class = "com.learn.Car" autowire="byType"/>
	



</beans>
